{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "assetBundlePatterns": ["./src/assets/fonts/*"],
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
   
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to provide services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to provide services.",
        "NSLocationAlwaysUsageDescription": "We need your location even when the app is in the background."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "ACCESS_BACKGROUND_LOCATION"]

    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
